{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "full_name": {
      "type": "string",
      "required": true
    },
    "phone_number": {
      "type": "string",
      "required": true
    },
    "profile_picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "address": {
      "type": "string"
    },
    "postcode": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::review.review",
      "inversedBy": "users"
    },
    "jobs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::job.job",
      "inversedBy": "users"
    },
    "is_email_verified": {
      "type": "boolean",
      "default": false
    },
    "is_phone_number_verified": {
      "type": "boolean",
      "default": false
    },
    "is_user_accessed": {
      "type": "boolean",
      "default": false
    },
    "registered_business_number": {
      "type": "string"
    },
    "portfolios": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::portfolio.portfolio",
      "inversedBy": "users"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "joined_on": {
      "type": "string",
      "required": true
    }
  }
}
