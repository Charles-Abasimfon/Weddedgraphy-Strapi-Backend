{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Full_name": {
      "type": "string"
    },
    "Email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "Phone_number": {
      "type": "string",
      "unique": true
    },
    "Profile_picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Address": {
      "type": "string"
    },
    "Latitude": {
      "type": "string"
    },
    "Longitude": {
      "type": "string"
    },
    "Postcode": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "Client"
    },
    "Jobs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::job.job",
      "mappedBy": "Client"
    },
    "Joined_on": {
      "type": "datetime",
      "required": true
    },
    "Password": {
      "type": "password",
      "required": true,
      "private": true
    }
  }
}
