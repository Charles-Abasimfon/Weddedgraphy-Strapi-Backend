{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Body": {
      "type": "richtext"
    },
    "Files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Date": {
      "type": "date"
    },
    "Contractor_name": {
      "type": "string"
    },
    "Contractor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::contractor.contractor",
      "inversedBy": "Portfolios"
    },
    "Added_on": {
      "type": "datetime",
      "required": true
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "portfolios"
    }
  }
}
